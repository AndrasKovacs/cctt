propIsContr (A : U) : prop (isContr A)
  := let rem (t : isContr A) : prop (isContr A)
       := let T (x : A) : U := (y : A) → y = x;
          let pA (x y : A) : x = y := λ i. hcom 1 0 [i=0 _. x; i=1. t.2 y] (t.2 x i);
          let pB (x : A) : prop (T x) := propPi A (λ y. y = x) (λ y. propSet A pA y x);
          propSig A T pA pB;
     lemProp (isContr A) rem;

propIsContrNf (A : U) : prop (isContr A)
  :=
λ a u i.
  ( hcom 1 0 A [i = 0 _. a.1; i = 1 j. a.2 u.1 j] (a.2 a.1 i)
  , λ x i1. hcom 0 1 A [
              i1 = 0 k. hcom 1 0 [k = 0 _. x; k = 1 j. a.2 x j] (a.2 x k)
	    ; i1 = 1 k. hcom 1 0 [k = 0 _. x
	                        ; k = 1 j. a.2 (hcom 1 0 [i = 0 _. a.1
				                        ; i = 1 j1. a.2 u.1 j1]
						        (a.2 a.1 i)) j] (a.2 x k)
	    ; i = 0 k. hcom 1 0 [k = 0 _. x; k = 1 j. a.2 (a.2 x i1) j] (a.2 x k)
	    ; i = 1 k. hcom 1 0 [k = 0 _. x; k = 1 j. a.2 (u.2 x i1) j] (a.2 x k)]
	    x);



